# Zoho OAuth Credentials (Required)
ZOHO_CLIENT_ID=your_client_id_here
ZOHO_CLIENT_SECRET=your_client_secret_here
ZOHO_REFRESH_TOKEN=your_refresh_token_here

# Zoho API Configuration (Optional - defaults shown)
ZOHO_API_DOMAIN=https://www.zohoapis.com
ZOHO_ACCOUNTS_URL=https://accounts.zoho.com

# Zoho CRM Module Configuration (Optional - defaults shown)
ZOHO_MODULE_NAME=Form_Submissions
ZOHO_SCORE_FIELD=Score
ZOHO_KEY_FIELD=Email

# Score Threshold (Optional - default: 70)
SCORE_THRESHOLD=70

# Redirect URLs (Required)
SUCCESS_REDIRECT_URL=https://calendly.com/your-link

# Optional: External thank you page URL (if not set, uses built-in page)
# THANK_YOU_PAGE_URL=https://yoursite.com/thank-you
